//
class task4 {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int[] arr = new int[10];
        for (int i = 0; i < 10; i++)
            arr[i] = s.nextInt();
        int max = arr[0];
        int min = arr[0];
        for (int i = 0; i < 10; i++) {
            if (arr[i] > max)
                max = arr[i];
            if (arr[i] < min)
                min = arr[i]; }
            System.out.println(differenceMaxMin(min, max));

    }
    public static int differenceMaxMin(int min, int max) {
        int difference = max - min;
        return difference;
    }
}
