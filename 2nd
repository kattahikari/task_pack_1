//
class task8 {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        String s1 = s.nextLine ();
        System.out.println(getXO(s1));
    }
    public static boolean getXO(String s1) {
        int x = 0;
        int o = 0;
        boolean xo = true;
        for (char i : s1.toCharArray()) {
            if (i == 'x')
                x++;
            if (i == 'o')
                o++; }
        if (x == o)
            xo = true;
        else
            xo = false;
        return xo;
    }
}
